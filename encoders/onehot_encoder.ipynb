   "source": [
    "# One Hot Encoder"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "16e700d2-dc26-4d32-9ccf-ab7aaab2a73f",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import OneHotEncoder"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "a29553f0-123a-41c9-b074-b3c6e8b9bfeb",
   "metadata": {},
   "outputs": [],
   "source": [
    "def one_hot_encoded(column, df):\n",
    "    encoder = OneHotEncoder()\n",
    "    encoder.fit_transform(df.loc[:, [column]]).toarray()\n",
    "    encoder.get_feature_names_out()\n",
    "    return encoder"
   ]
  },
