   "outputs": [],
   "source": [
    "from sklearn.preprocessing import OrdinalEncoder"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "316bcb77-7a36-49c3-acb0-ed07115e4415",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ordinal_encoded(column, df, categories):\n",
    "    encoder = OrdinalEncoder(categories= [categories])\n",
    "    return encoder.fit_transform(df.loc[:, [column]])"
   ]
  },
