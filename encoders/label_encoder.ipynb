   "source": [
    "from sklearn.preprocessing import LabelEncoder"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "5e8edf65-09d9-4afc-a359-37d534246443",
   "metadata": {},
   "outputs": [],
   "source": [
    "def label_encoded(column, df):\n",
    "    encoder = LabelEncoder()\n",
    "    df[column] = encoder.fit_transform(df[column])\n",
    "    return df[column]"
   ]
  },
